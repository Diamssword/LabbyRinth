{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile dist/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile dist/smui-dark.css -i src/theme/dark",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@smui/button": "^7.0.0-beta.18",
    "@smui/layout-grid": "^7.0.0-beta.18",
    "@smui/list": "^7.0.0-beta.18",
    "@smui/menu-surface": "^7.0.0-beta.18",
    "@smui/paper": "^7.0.0-beta.18",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "smui-theme": "^7.0.0-beta.18",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-zip-pack": "^1.2.3"
  },
  "dependencies": {
    "svelte-material-ui": "^7.0.0-beta.18"
  }
}
